<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GenderToggle">
        <attr name="toggle_indicator_color" format="color"/>
        <attr name="toggle_indicator_radius" format="integer"/>
        <attr name="toggle_root_radius" format="integer"/>
        <attr name="toggle_root_radius_width" format="integer"/>
        <attr name="toggle_root_radius_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="TwitterSplashView">
        <attr name="icon" format="reference"/>
        <attr name="iconColor" format="color"/>
        <attr name="holeFillColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="removeFromParentOnEnd" format="boolean"/>
    </declare-styleable>
</resources>